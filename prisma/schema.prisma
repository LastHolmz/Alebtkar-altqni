datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Demo {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  image       String
  price       Int?
  title       String
  href        String
  bio         String
  description String?
}

model Contact {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  fullName  String
  phone     Int
  content   String
  email     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
